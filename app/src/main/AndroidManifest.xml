<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.notesandreminding">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/fav"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".NewPassActivity"
            android:label="@string/_set"></activity>
        <activity
            android:name=".WriteNotesActivity"
            android:label="@string/new_note"></activity>
        <activity
            android:name=".NotesAct"
            android:label="@string/title_activity_notes"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>